	<insertafter xpath="/windows/window[@name='windowQuestDescription']">
		<window name="windowQuestObjectives" width="533" height="30" panel="Center" controller="QuestObjectivesWindow" cursor_area="true" >
 	<!--#$-IGS END.-$#-->
		<panel name="header" pos="1,-44" height="40" depth="1" backgroundspritename="ui_game_panel_header">
			<sprite depth="2" name="windowIcon" style="icon32px" pos="4,-5" sprite="ui_game_symbol_quest"/>
			<label style="header.name" text="OBJECTIVES" text_key="xuiObjectives" />
			
			<!-- <label depth="2" pos="445,-8" width="200" height="28" text="{tieradd}" justify="right" upper_case="true" /> -->
		</panel>
		<rect name="content" pos="1,-84" height="40" depth="1">
			<sprite depth="2" name="backgroundMain" sprite="menu_empty3px" color="[black]" type="sliced" fillcenter="false" />
			<sprite color="[darkGrey]" type="sliced" />
			<rect visible="{showquest}">
				<grid name="objectiveList" rows="5" cols="2" pos="12,-10" width="300" height="40" cell_width="300" cell_height="30" repeat_content="true" arrangement="horizontal" controller="QuestObjectiveList">
					<quest_objective_entry name="0" />
				</grid>
				
				<label depth="3" pos="45,-8" text="{questrequirementstitle}: {questrequirements}" width="290" visible="{showrequirements}" height="30" />
			</rect>
			<rect visible="{showempty}">
				<label pos="0,-70" depth="3" name="descriptionText" width="100%" height="65" text="OBJECTIVES NOT AVAILABLE AT THIS TIME." text_key="xuiObjectivesNotAvailable" justify="center" font_size="28" />
			</rect>
		</rect>
		</window>	
	</insertafter>
	
	<remove xpath="/windows/window[@name='windowQuestRewards']"/>
	
