	<buff name="buffTimeREvert" hidden="true">
		<stack_type value="replace"/>
		<duration value="0"/>
		<display_value value=".buffTimeREvertDisplay"/>
		<display_value_format value="time"/>

		<effect_group>
			<triggered_effect trigger="onSelfBuffStart" action="ModifyCVar" cvar=".buffTimeREvertDisplay" operation="set" value="115"/>
			<triggered_effect trigger="onSelfBuffStart" action="ModifyCVar" cvar="$buffTimeREvertDuration" operation="set" value="@.buffTimeREvertDisplay"/>
			<triggered_effect trigger="onSelfBuffUpdate" action="ModifyCVar" cvar="$buffTimeREvertDuration" operation="subtract" value="1"/>
			<triggered_effect trigger="onSelfBuffUpdate" action="ModifyCVar" cvar=".buffTimeREvertDisplay" operation="set" value="@$buffTimeREvertDuration"/>
		</effect_group>
		
		<effect_group>
			<triggered_effect trigger="onSelfBuffUpdate" action="CallGameEvent" event="reverse_Time" />
				<requirement name="CVarCompare" cvar="$buffTimeREvertDuration" operation="LTE" value="0"/>
		</effect_group>
		
		<effect_group>
		<triggered_effect trigger="onSelfBuffUpdate" action="RemoveBuff" buff="buffTimeREvert">
				<requirement name="CVarCompare" cvar="$buffTimeREvertDuration" operation="LTE" value="0"/>
		</triggered_effect>
		</effect_group>
		
		<effect_group>
		<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" buff="buffTimeREvert2">
				<requirement name="CVarCompare" cvar="$buffTimeREvertDuration" operation="LTE" value="0"/>
		</triggered_effect>
		</effect_group>
	</buff>	
	
