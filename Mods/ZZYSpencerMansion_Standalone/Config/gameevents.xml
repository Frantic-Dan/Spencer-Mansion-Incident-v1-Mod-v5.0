<append xpath="/gameevents">

	<action_sequence name="EventClassicModeJill">
		<property name="action_type" value="Game" />

		<action class="SetItemSlots">
			<property name="items_location" value="Toolbelt" />
			<property name="items" value="ClassicModeInventoryBlocker,ClassicModeInventoryBlocker" />
			<property name="item_counts" value="1,1" />
			<property name="slot_numbers" value="8,9" />
		</action>
	</action_sequence>

		<action_sequence name="EventClassicModeChris">
		<property name="action_type" value="Game" />

		<action class="SetItemSlots">
			<property name="items_location" value="Toolbelt" />
			<property name="items" value="ClassicModeInventoryBlocker,ClassicModeInventoryBlocker,ClassicModeInventoryBlocker,ClassicModeInventoryBlocker" />
			<property name="item_counts" value="1,1,1,1" />
			<property name="slot_numbers" value="6,7,8,9" />
		</action>
	</action_sequence>
		
	<action_sequence name="action_FuelSupplyCapsule_Explode">
		<property name="action_type" value="Game" />

			<action class="PlaySound">
				<property name="sound" value="twitch_countdown_tick" />
				<property name="phase" value="1" />
			</action>
			
			<action class="Delay">
				<property name="time" value="1" />
			</action>
			
			<action class="RemoveItems">
				<property name="items_location" value="Toolbelt" />
				<property name="items_tags" value="FuelSupplyCapsule"/>
				<property name="count_type" value="Items"/>
				<property name="count" value="1"/>			
			</action>
		
		<action class="ExplodeTarget">
			<property name="phase" value="2" />
			<property name="blast_power" value="75" />
			<property name="entity_damage" value="50000" />
			<property name="entity_radius" value="6" />
			<!-- <property name="target_group" value="targets" /> -->
		</action>
		
	</action_sequence>

	<action_sequence name="reverse_Time">
		<property name="action_type" value="Game" />
		
		<action class="SetDayTime" >
			<property name="day" value="-1" />
			<property name="hours" value="1" />
			<property name="minutes" value="1" />
		</action>	
	</action_sequence>	
	
	<action_sequence name="BeatGame">
		
		<action class="Delay">
			<property name="time" value="4" />
			<property name="phase" value="1" />
		</action>
		
		<action class="AddBuff">
			<property name="phase" value="2" />
			<property name="buff_name" value="buffDontMove" />
			<property name="duration" value="300" />
		</action>
		
		<action class="AddBuff">
			<property name="phase" value="2" />
			<property name="buff_name" value="buffInvincibilityFrames" />
			<property name="duration" value="300" />
		</action>
		
		<action class="SetScreenEffect">
			<property name="phase" value="3" />
			<property name="screen_effect" value="FadeToBlack" />
			<property name="intensity" value="1" />
			<property name="fade_time" value="4" />
		</action>
		
		<action class="Delay">
			<property name="time" value="5" />
			<property name="phase" value="4" />
		</action>
		
		<action class="ShowMessageWindow" >
			<property name="message" value="Completion_desc"/>
			<property name="title" value="Quest_Completion_tip_title"/>
			<property name="phase" value="5"/>
		</action>
	
	</action_sequence>
	
	<action_sequence name="You are a cheater">
		
		<!-- <action class="SetScreenEffect">
			<property name="phase" value="1" />
			<property name="screen_effect" value="FadeToBlack" />
			<property name="intensity" value=".7" />
			<property name="fade_time" value="5400" />
		</action> -->
		
		<action class="Delay">
			<property name="time" value="3600" />
			<property name="phase" value="1" />
		</action>
		
		<action class="BeltTooltip">
			<property name="phase" value="2" />
			<property name="text" value="[FF4F00]Did you cheat in a light source?[-]" />
		</action>
		
		<!-- <action class="BeltTooltip">
			<property name="phase" value="4" />
			<property name="text" value="[FF4F00]It sure is dark in this game, hey!?[-]" />
		</action>
		
		<action class="Delay">
			<property name="time" value="8" />
			<property name="phase" value="5" />
		</action> -->
		
		<action class="AddBuff">
			<property name="phase" value="3" />
			<property name="buff_name" value="Tarnished Integrity" />
		</action>
		
		<!-- <action class="Delay">
			<property name="time" value="6" />
			<property name="phase" value="7" />
		</action>
		
		<action class="BeltTooltip">
			<property name="phase" value="8" />
			<property name="text" value="[FF4F00]Ahhh yes, that's much better![-]" />
		</action>
		
		<action class="BeltTooltip">
			<property name="phase" value="9" />
			<property name="text" value="[FF4F00]Enjoy your cheated light source now![-]" />
		</action>
		
		<action class="Delay">
			<property name="time" value="2700" />
			<property name="phase" value="10" />
		</action>
		
		<action class="BeltTooltip">
