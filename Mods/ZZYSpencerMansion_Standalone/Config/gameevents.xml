			<property name="title" value="Spencer Mansion Incident" />
			<property name="phase" value="1" />
		</action>

		<action class="Delay">
			<property name="time" value="1" />
			<property name="phase" value="2" />
		</action>

		<action class="SetItemSlots">
				<property name="items_location" value="Equipment" />
				<property name="items" value="armorJillOutfit,armorJillGloves,armorJillBoots" />
				<property name="item_counts" value="1-1,1-1,1-1" />
			</action>

		<action class="AddQuest">
			<property name="quest" value="SpencerMansionQuest" />
			<property name="phase" value="3" />
		</action>
		
		<action class="Delay">
			<property name="time" value="1" />
			<property name="phase" value="4" />
		</action>
		
		<action class="AddBuff">
			<property name="phase" value="5" />
			<property name="buff_name" value="I probably should not cheat" />
		</action>
		
